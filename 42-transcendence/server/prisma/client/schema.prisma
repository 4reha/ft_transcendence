generator client {
  provider = "prisma-client-js"
  output   = "./client"

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player{
  id                Int       @id @default(autoincrement())
  email             String    @unique 
  nickname          String    @unique
  avatar            String?
  joinAt            DateTime  @default(now())

}
